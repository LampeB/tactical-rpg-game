config_version=5

[application]

config/name="Tactical RPG"
config/description="A tactical RPG with grid-based inventory and turn-based combat"
run/main_scene="res://scenes/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.6", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

DebugLogger="*res://scripts/autoload/debug_logger.gd"
EventBus="*res://scripts/autoload/event_bus.gd"
ItemDatabase="*res://scripts/autoload/item_database.gd"
CharacterDatabase="*res://scripts/autoload/character_database.gd"
PassiveTreeDatabase="*res://scripts/autoload/passive_tree_database.gd"
GameManager="*res://scripts/autoload/game_manager.gd"
SaveManager="*res://scripts/autoload/save_manager.gd"
SceneManager="*res://scripts/autoload/scene_manager.gd"
InputManager="*res://scripts/autoload/input_manager.gd"
UIColors="*res://scripts/autoload/ui_colors.gd"
NpcDatabase="*res://scripts/autoload/npc_database.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[gui]

theme/custom="res://assets/ui/default_theme.tres"

